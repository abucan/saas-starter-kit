{
  "name": "saas-starter-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:app": "next dev",
    "dev:stripe": "stripe listen --events checkout.session.completed,customer.subscription.created,customer.subscription.updated,customer.subscription.deleted --forward-to http://localhost:3000/api/auth/stripe/webhook",
    "dev:all": "concurrently -k -n next,stripe -c auto \"npm:dev:app\" \"npm:dev:stripe\"",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "email:dev": "email dev --dir src/lib/email/templates -p 3001"
  },
  "dependencies": {
